{
  "name": "@chainlink/covalent-adapter",
  "version": "1.0.0",
  "description": "Chainlink Covalent adapter.",
  "keywords": [
    "Chainlink",
    "LINK",
    "blockchain",
    "Covalent"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/smartcontractkit/external-adapters-js",
    "type": "git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "prepack": "yarn build",
    "build": "tsc -b",
    "server": "node -e 'require(\"./index.js\").server()'",
    "server:dist": "NODE_ENV=development node -e 'require(\"./dist/index.js\").server()'",
    "start": "yarn server:dist"
  },
  "dependencies": {
    "@chainlink/ea-bootstrap": "workspace:*",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.7",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@chainlink/types": "workspace:*",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.36",
    "typescript": "4.3.5"
  }
}
